
parse def foo(str)
{
	stmt : mul
			%c{
				print: '1: ', argv;
				return argv[0];
			}
		;

	mul : mul '*' number
			%c{
				print: '2: ', argv;
				return argv[0] * argv[2];
			}
		| number
			%c{
				print: '3: ', argv;
				return argv[0];
			}
		;

	number : r'[0-9]+'
			%c{
				print: '4: ', argv;
				return argv[0].integer();
			}
		;
}

print: 'start';
#print: my.foo('1');
print: my.foo('1*2*3');
print: 'done';


