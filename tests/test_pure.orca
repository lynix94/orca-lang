
using stopwatch;


def fibo(n) 
{
	if n == 0 {
		return 0;
	}
	
	if n == 1 {
		return 1;
	}

	return fibo(n-1) + fibo(n-2);
}

stopwatch.start();
print: f1 = my.fibo(24);
print: t1 = stopwatch.elapse();
stopwatch.lap();


@pure
def pure_fibo(n)
{
	if n == 0 {
		return 0;
	}
	
	if n == 1 {
		return 1;
	}

	return pure_fibo(n-1) + pure_fibo(n-2);
}
	
stopwatch.start();
print: f2 = my.pure_fibo(24);
print: t2 = stopwatch.elapse();
stopwatch.lap();

if f1 != f2 || t1 <= t2 {
	throw test.test_pure, 'pure result fail';
}
	

print: 'OK';

