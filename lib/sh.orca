
using os;

return my.script.clone(argv[2]); 

def script(...)
{
  new_cmd = .cmd;
  for i in [0..argv.size()-1] {
	new_cmd = new_cmd.replace('argv[%d]'%i, argv[i]);
  }

  new_cmd = new_cmd.replace('argc', argv.size());

  fr = new_cmd.find(r'argv[^\[]');
  new_cmd = new_cmd[:fr[1]] + argv + new_cmd[fr[2]-1:];

  print: new_cmd;
  os.run(new_cmd);
  return;

  def init(input)
  {
	..cmd = input;
  }

  def cmd := '';
}




